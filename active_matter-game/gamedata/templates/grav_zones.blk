indoor_grav_zone{
  _singleton:b=yes
  _use:t="replicating"
  _use:t="gravity_zone_box"
  _replicated:t="transform"
  transform:m=[[1, 0, 0] [0, 1, 0] [0, 0, 1] [0, 0, 0]]
  _replicated:t="gravity_zone__boxSize"
  gravity_zone__boxSize:p3=1, 1, 1
  indoor_grav_zone__duration:r=0
  indoor_grav_zone__timeLeft:r=0
  _replicated:t="indoor_grav_zone__timeLeft"
  _replicated:t="indoor_grav_zone__duration"

  _group{
    _tags:t="server"
    indoor_grav_zone__visualTempl:t="indoor_euclid_central_sphere"
    indoor_grav_zone__curPlaceIdx:i=-1
    indoor_grav_zone__minMaxDuration:p2=10, 120

    "indoor_grav_zone_mgr__places:array"{}
  }

  "indoor_grav_zone__visualEid:eid"{}
}

