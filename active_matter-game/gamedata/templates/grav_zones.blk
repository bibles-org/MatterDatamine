gravity_zone_base{
  _use:t="replicating"
  _use:t="gravity_zones_static_grid_object"
  grav_zone__itm:m=[[1, 0, 0] [0, 1, 0] [0, 0, 1] [0, 0, 0]]
  grav_zone__precalcScale:p3=1, 1, 1
  transform:m=[[1, 0, 0] [0, 1, 0] [0, 0, 1] [0, 0, 0]]
  _tracked:t="transform"
  grav_zone__type:i=0
  grav_zone__isImportant:b=no
  grav_zone__level:i=1
  grav_zone__weight:r=1

  "net__alwaysInScope:tag"{}
}

