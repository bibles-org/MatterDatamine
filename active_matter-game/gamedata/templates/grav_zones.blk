gravity_path_box{
  _use:t="gpu_object_placer_box"
  _use:t="gravity_zone_base"
  gravity_zone__boxSize:p3=1, 1, 1
  grav_zone__boxBorderX:p2=0, 0
  grav_zone__boxBorderY:p2=0, 0
  grav_zone__boxBorderZ:p2=0, 0
  transform:m=[[1, 0, 0] [0, 1, 0] [0, 0, 1] [0, 0, 0]]
  ri_gpu_object__name:t="gravity_well_crystals"

  _group{
    _tags:t="render"
    _track:b=yes
    gpu_object_placer__ri_asset_idx:i=-1
    gpu_object_placer__object_density:r=5
    gpu_object_placer__min_scale_radius:r=100
    gpu_object_placer__object_up_vector_threshold:p4=0, 0, 0, 0.1
    gpu_object_placer__current_distance_squared:r=0
    gpu_object_placer__filled:b=no
    gpu_object_placer__buffer_offset:i=-1
    gpu_object_placer__buffer_size:i=-1
    gpu_object_placer__show_geometry:b=no
    gpu_object_placer__place_on_geometry:b=yes
    gpu_object_placer__min_gathered_triangle_size:r=0.01
    gpu_object_placer__opaque:b=yes
    gpu_object_placer__decal:b=yes
    gpu_object_placer__use_strict_decal_placement:b=yes
  }

  _group{
    _track:b=yes
    gpu_object_placer__distance_based_scale:p2=1.2, 0.4
    gpu_object_placer__object_scale_range:p2=0.5, 2
    gpu_object_placer__object_max_count:i=400
    gpu_object_placer__visible_distance_squared:r=10000
  }
}

