projectile_inactive{
  projectile__timeToDestroy:r=1

  "projectile__inactive:tag"{}
}

disable_ricochet{
  projectile__sceneHitRicochetStability:r=1
}

bullet_projectile{
  _use:t="ecs_no_debug"
  _use:t="bullet_projectile_impact_sound"
  detonation__timer:r=-1
  projectile__lifeTime:r=0
  projectile__visualPos:p3=0, 0, 0
  projectile__minLifeTime:r=0
  projectile__maxLifeTime:r=0
  projectile__stopped:b=no
  projectile__exploded:b=no
  projectile__prevPos:p3=0, 0, 0
  projectile__intersectionCount:i=0
  projectile__maxAllowedSceneIntersectionsForFx:i=3
  projectile__traceRayMatId:i=-1
  projectile__isUnderwater:b=no
  projectile__hitEffectDescriptor:i=0
  projectile__explEffectDescriptor:i=0
  projectile__fleshEffectCaliberGroup:r=-1
  projectile__pierceShiftOnStartPenetration:r=0
  projectile_trace_info__sceneIntersected:b=no
  projectile_trace_info__pmid:i=-1
  projectile_trace_info__dir:p3=0, 0, 0
  projectile_trace_info__len:r=0
  projectile_trace_info__norm:p3=0, 0, 0
  projectile_trace_info__prevProjectileDistance:r=0
  projectile_trace_info__waterLen:r=0
  projectile_trace_info__riDescCellIdx:i=-1
  projectile_trace_info__riDescIdx:i=-1
  projectile_trace_info__riDescPool:i=-1
  projectile_trace_info__riDescLayer:i=-1
  scene_hit_place__pos:p3=0, 0, 0
  scene_hit_place__norm:p3=0, 1, 0
  scene_hit_place__matId:i=-1
  projectile_hit_fx__fleshPresetIndex:i=-1
  loud_noise__radius:r=15
  loud_noise__noisePerHit:r=80

  "projectile_trace_info__riDescOffs:u32"{
    value:i=-1
  }

  "dm_projectile_state:dm::ProjectileState"{}
  "projectile_ballistics:ballistics::ProjectileBallistics"{}
  "projectile_ballistics_props:ballistics::ProjectileProps"{}
  "launch_desc:daweap::LaunchDesc"{}
  "ownerEid:eid"{}
  "gunEid:eid"{}
  "projectile__penetratedEids:list<eid>"{}
  "projectile__lastDamagedRiexHandle:u64"{
    value:i64=-1
  }

  _group{
    _tags:t="render"
    projectile_visual_tracer__id:i=-1
    projectile_visual_tracer__ignitionTime:r=0.02
    projectile_visual_tracer__burnTime:r=1.1
    projectile_visual_tracer__smokeTime:r=1
    projectile_visual_tracer__smokeRad:r=0.01
    projectile_visual_tracer__luminosity:r=10
    projectile_visual_tracer__smokeTim:r=1
    projectile_visual_tracer__smokeColor:p4=1, 0.9, 0.7, 0.8
    projectile_visual_tracer__headColor:p3=1, 0.666, 0.333
    projectile_visual_tracer__startHeadColor:p3=1, 0.666, 0.333
  }
}

