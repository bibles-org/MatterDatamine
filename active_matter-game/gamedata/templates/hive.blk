  "beehive:tag"{}
  "hive_body__templates:array"{
    "item:object"{
      template:t="hive_dead_body_container_corrupted_male_a"
      weight:r=5
    }

    "item:object"{
      template:t="hive_dead_body_container_corrupted_male_b"
      weight:r=5
    }

    "item:object"{
      template:t="hive_dead_body_empty_a"
      weight:r=1
    }

    "item:object"{
      template:t="hive_dead_body_empty_d"
      weight:r=1
    }
  }

  "hive_honeycomb__templates:array"{
    "item:object"{
      template:t="hive_enhanced_honeycomb_piece"
      number:ip2=1, 1
    }
  }

  "loot_drop_system__dropRequestOnRiDestruction:tag"{}
  _group{
    _tags:t="gameClient"
    hive_client_destruction__effectTemplate:t="hive_wasp_slime_leaks_effect"
    hive_client_destruction__effectOffset:p3=-3.25, -2, 0
    hive_client_destruction__fallingEffectMarkTemplate:t="hive_wasp_slime_falling_mark"
    hive_client_destruction__fallingSoundTemplate:t="hive_falling_sound"
  }
}

