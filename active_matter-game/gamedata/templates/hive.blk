beehive{
  _use:t="damageable_rendinst"
  ri_extra__name:t="hive_wasp_wall_a"
  transform:m=[[1, 0, 0] [0, 1, 0] [0, 0, 1] [0, 0, 0]]
  ri_preload__name:t="hive_wasp_wall_a_dmg"
  hive_body__destructionTriggered:b=no
  hive_body__spawnChance:r=0.4
  hive_body__spawnOffset:p3=-3.5, -4, 0
