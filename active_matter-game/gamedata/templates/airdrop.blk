  _group{
    _tags:t="server"
    airdrop_manager__initialTimer:p2=240, 300
    airdrop_manager__repeatedTimer:p2=180, 240
    airdrop_manager__quantityRangeAirdrop:ip2=4, 5
    airdrop_manager__airdropTemplate:t="airdrop_container_magazines"
  }
