  "slot_attach__attached_show_dynmodel_nodes__forceShownNodes:list<t>"{
    item:t="pouches_1"
  }

  "equipment_mods__slots:shared:object"{
    equipment_mod_pocket_0:t="pocket_slot"
  }
}

