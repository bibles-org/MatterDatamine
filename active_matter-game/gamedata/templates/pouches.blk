pouch_base{
  _use:t="equipable_item"
  _use:t="equipment_with_mods"
  item__recreateOnGround:t="item_in_world"
  item__recreateInWorldPhys:t="item_in_world_phys"
  item__recreateInInventory:t="base_invis_item"
  item__recreateInEquipment:t="base_attached_invis_item"
  item__filterType:t="equipment"
  collres__res:t="medkit_collision"
  profile_server_data__enrichmentRefineChronotracesRange:ip2=5, 5
  item__nexusCost:i=25

  "item__equipmentSlots:list<t>"{
    slot:t="pouch"
  }

  "equipment_item:tag"{}
  "item__chronotracesProgressionInfo:object"{
    "repeatedly:object"{
      extract:i=100
    }
  }
}

