player_hits_counter{
  _group{
    _tags:t="server"
    hits_counter__verified:i=0
    hits_counter__failed:i=0
  }
}

player_ribbons{
  _group{
    _track:b=yes
    _replicate:b=yes
    player_ribbons__curColors:ip2=-1, -1
  }

  _group{
    _tags:t="server"
    player_ribbons__rndColors:b=no
  }
}

player{
  _use:t="replicating"
  _use:t="player_hits_counter"
  _use:t="player_ribbons"
  _use:t="dynamic_chevron"
  connid:i=-1
  platformUid:t=""
  platform:t=""
  countAsAlive:b=yes
  appId:i=0
  clientNetFlags:i=0

  player{}
  "net__notInInitialScope:tag"{}
  _group{
    _track:b=yes
    _replicate:b=yes
    name:t=""
    scoring_player__firstSpawnTime:r=-1
    scoring_player__playTime:p2=0, 0
    scoring_player__medicineUsed:i=0
    scoring_player__lootedSecretContainers:i=0
    scoring_player__lootedCommonContainers:i=0
    scoring_player__lootedBodies:i=0
    scoring_player__receivedDamage:r=0
    scoring_player__dealtDamage:r=0
    scoring_player__playerPathLen:r=0
    player__isFinishedRaid:b=no
    scoring_player__isExtractedSuccess:b=no
    player__spawnCount:i=0
    groupId:i64=0
    team:i=-1
    disconnected:b=no

    "player__stats:object"{}
    "player__teamEid:eid"{}
    "possessed:eid"{}
    "specTarget:eid"{}
  }

  _group{
    _tags:t="server"
    startedAtTime:r=-1
    connectedAtTime:r=-1
    disconnectedAtTime:r=-1
    receive_logerr:b=no
    player__canBeLocal:b=yes

    "player__usedContainers:list<eid>"{}
    "killEntityTimer:u64"{
