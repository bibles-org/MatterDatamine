  _group{
    _tags:t="gameClient"
    activeCamName:t=""

    "activeCamEid:eid"{}
  }

  "player_actions_log:list<t>"{}
  _group{
    _tags:t="gameClient"
    _track:b=yes
    is_local:b=no
  }
}

