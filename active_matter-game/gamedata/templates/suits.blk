  slot_attach__visible:b=yes
  _tracked:t="slot_attach__visible"
  _replicated:t="slot_attach__visible"
  animchar__res:t=""

  "suit_attachable_animchar:tag"{}
}

suit_attachable_animchar{
  _use:t="replicating"
