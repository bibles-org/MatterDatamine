attachable_suit_controller{
  attachable_suit_controller__suitType:i=0
  entity__deathTemplate:t="update_attachables_post_ragdoll"
  _tracked:t="attachable_suit_controller__equipmentAttachedAnimchars"

  _group{
    _track:b=yes
    _replicate:b=yes

    "attachable_suit_controller__suitAnimcharEid:eid"{}
    "attachable_suit_controller__attachedAnimcharEids:object"{}
  }

  "attachable_suit_controller__mainAnimcharConfig:array"{}
  "attachable_suit_controller__attachedAnimcharTemplates:object"{}
  "attachable_suit_controller__holsterAttachedAnimchars:list<t>"{
    EWS_PRIMARY:t=""
    EWS_SECONDARY:t=""
    EWS_TERTIARY:t=""
    EWS_MELEE:t=""
    EWS_GRENADE:t=""
  }

  "attachable_suit_controller__equipmentAttachedAnimchars:object"{}
}

nonreplicated_suit_attachable_animchar{
  _use:t="nonreplicated_human_animchar_attachable_part"
