am_projectile_hit_vehicle_fx_sound{
  "projectile_hit_vehicle_fx_sound__paths:shared:object"{
    _tags:t="sound"

    "stopped:object"{
      "bullet:object"{
        hero:t="player/vehicle/tank/impacts/hit_small_fail"
        enemy:t="enemy/vehicle/tank/impacts/hit_small_fail"
      }

      "shell:object"{
        hero:t="player/vehicle/tank/impacts/hit_mid_fail"
        enemy:t="enemy/vehicle/tank/impacts/hit_mid_fail"
      }
    }

    "unstopped:object"{
      "bullet:object"{
        hero:t="player/vehicle/tank/impacts/hit_small_fail"
        enemy:t="enemy/vehicle/tank/impacts/hit_small_fail"
      }

      "shell:object"{
        hero:t="player/vehicle/tank/impacts/hit_mid"
        enemy:t="enemy/vehicle/tank/impacts/hit_mid"
      }
    }
  }
}

am_car_vehicle_sound{
  _use:t="car_vehicle_sound"
  _use:t="am_projectile_hit_vehicle_fx_sound"
}

