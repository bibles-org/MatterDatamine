  "moving_zone_hud:tag"{
    _tags:t="render"
  }

  "moving_zone__generations:array"{
    _hide:b=yes
    _tags:t="server"
  }

  "moving_zone__generated:b"{
    _tags:t="server"
    value:b=no
    _hide:b=yes
  }
}

