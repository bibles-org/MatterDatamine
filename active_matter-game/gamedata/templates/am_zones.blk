anomaly_zone{
  transform:m=[[1, 0, 0] [0, 1, 0] [0, 0, 1] [0, 0, 0]]
  nmesh_voxel__randomSeed:i=0
  nmesh_voxel__gridStep:r=2
  nmesh_voxel__gridSize:ip3=40, 18, 40
  nmesh_voxel__debugDraw:b=no
  nmesh_voxel__debugDrawBranches:b=no
  nmesh_voxel__debugTime:i=1000
  anomaly_zone__generated:b=no
  anomaly_zone__dryRun:b=no
  anomaly_zone__branchesGrowIterations:i=30
  anomaly_zone__brancheLengthMult:r=1
  anomaly_zone__spawnCountMult:r=1
  anomaly_zone__spawnTraitTemplate:t="zone_child"
}

zone_child{
  "anomaly_zone__ownerZoneEid:eid"{}
}

anomaly_zone_branch{
  anomaly_zone__branchLength:ip2=10, 20
}

flowerman_anomaly_gpu_objects_trait{
  _group{
    _tags:t="render"
    gpu_object_placer__object_density:r=1.5
  }
}

gpu_objects_trait_low_density{
  _group{
    _tags:t="render"
    gpu_object_placer__object_density:r=0.75
  }
}

anomaly_zone_confusion_branch{
  _use:t="anomaly_zone_branch"
  anomaly_zone__branchLength:ip2=100, 100

  "anomaly_zone__spawnPresets:object"{
    "anomaly_preset:array"{
      "it:object"{
        template:t="indoor_confusion_anomaly_trap+area_decor_confusion"
        scaleToBox:b=yes
      }
    }

    "decor_preset:array"{
      "it:object"{
        template:t="area_decor_confusion"
        scaleToBox:b=yes
      }
    }

    "artifact_legendary_preset:array"{
      "it:object"{
        template:t="artifact_legendary+alarm_on_pickup+item_in_world"
        projectToNavmesh:b=yes
      }

      "it:object"{
        template:t="area_decor_confusion"
        scaleToBox:b=yes
      }
    }

    "artifact_active_mattery_preset:array"{
      "it:object"{
        template:t="active_matter_pile+coin_item_count_10+item_in_world"
        projectToNavmesh:b=yes
      }

      "it:object"{
        template:t="area_decor_confusion"
        scaleToBox:b=yes
      }
    }

    "artifact_active_mattery_extraction_preset:array"{
      "it:object"{
        template:t="artifact_active_mattery_base"
        projectToNavmesh:b=yes
      }
    }

    "active_mattery_pile_preset:array"{
      "it:object"{
        template:t="active_matter_pile+coin_item_count_5+item_in_world"
        projectToNavmesh:b=yes
      }
    }
  }

  "anomaly_zone__spawnGroups:array"{
    "group:object"{
      spawnCount:ip2=4, 8
      preset:t="active_mattery_pile_preset"
      cellChancePow:r=1
    }

    "group:object"{
      spawnCount:ip2=2, 3
      preset:t="artifact_legendary_preset"
      cellChancePow:r=1
    }

    "group:object"{
      spawnCount:ip2=6, 6
      zoneMinSize:ip3=3, 3, 3
      preset:t="anomaly_preset"
    }

    "group:object"{
      spawnCount:ip2=16, 16
      zoneMinSize:ip3=3, 2, 3
      preset:t="anomaly_preset"
    }
  }
}

