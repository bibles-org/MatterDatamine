  moving_zone__sleepingTimeDecrease:r=10

  _group{
    _replicate:b=yes
    moving_zone__startEndTime:p2=0, 0
    moving_zone__sourcePos:p3=0, 0, 0
    moving_zone__targetPos:p3=0, 0, 0
    moving_zone__sourceRadius:r=0
    moving_zone__targetRadius:r=0
    moving_zone__collapseRadius:r=0.001
    moving_zone__collapseTime:r=10
    moving_zone__manual_activation:b=no
  }

  _group{
    _tags:t="server"
    moving_zone__isInitialized:b=no
    moving_zone__chooseFinalExtraction:b=yes
  }

  _group{
    _replicate:b=yes
    _track:b=yes
    moving_zone_hud__playerZonePosition:i=0
    moving_zone_hud__zoneStartTimer:i=-1
    moving_zone_hud__zoneFinishTimer:i=-1
    moving_zone_hud__zoneProgress:i=-1
    transform:m=[[1, 0, 0] [0, 1, 0] [0, 0, 1] [0, 0, 0]]
