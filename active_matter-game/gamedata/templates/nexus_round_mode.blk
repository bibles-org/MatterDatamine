nexus_round_mode_game_controller{
  _use:t="replicating"
  _use:t="nexus_game_controller"
  _use:t="nexus_round_mode"
  _use:t="nexus_round_mode_points_game_rules"
  _use:t="nexus_round_mode_lock_shooting_before_round_start"
  _use:t="nexus_round_mode_keep_protect_spheres_before_round_start"
  nexus_round_mode_game_controller__roundsToWin:i=3
  nexus_round_mode_game_controller__maxDrawRoundsInRow:i=3

  _group{
    _tags:t="server"
    nexus_round_mode_game_controller__defaultType:t="points_advantage"
    nexus_round_mode_game_controller__preparationTime:r=15
    nexus_round_mode_game_controller__delayAfterRoundFinish:r=5
    nexus_round_mode_game_controller__roundTime:r=240
    nexus_round_mode_game_controller__debriefingTime:r=5
    nexus_round_mode_game_controller__drawsInRow:i=0
  }

  _group{
    _track:b=yes
    _replicate:b=yes
    nexus_round_mode_game_controller__roundNumber:i=1
    nexus_round_mode_game_controller__roundState:i=0
    nexus_round_mode_game_controller__roundStateEndAt:r=-1
  }

  _group{
    _replicate:b=yes
    nexus_round_mode_game_controller__timeOutAt:r=-1
  }
}

nexus_round_mode_points_game_rules{
  _use:t="nexus_round_mode_points_scaling_sqrt"
  nexus_round_mode_game_controller__pointsToWin:r=100

  _group{
    _tags:t="server"
    nexus_round_mode_game_controller__pointsByKill:r=5
    nexus_round_mode_game_controller__pointsByTick:r=1
    nexus_round_mode_game_controller__pointsTickTimeDefault:r=1
    nexus_round_mode_game_controller__pointsTickTimeFast:r=0.1
  }
}

nexus_round_mode_points_game_controller{
  _tags:t="server"
  _use:t="nexus_round_mode_points"
  nexus_round_mode_game_controller__pointsTickTime:r=-1
  nexus_round_mode_game_controller__pointsNextTickAt:r=-1
  nexus_round_mode_game_controller__pointsDisplayTickTime:r=1
  nexus_round_mode_game_controller__pointsDisplayNextTickAt:r=-1
