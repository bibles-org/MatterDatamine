  _tags:t="render"
  weightsPath:t="gamedata/motion_matching/changed_civilian_mm_attack_weights.blk"
  rootMotionPath:t="gamedata/motion_matching/main_data_base_root_motion.blk"

  "data_bases_paths:list<t>"{
    path:t="gamedata/motion_matching/changed_civilian_mm_idle_anim_list.blk"
    path:t="gamedata/motion_matching/changed_civilian_mm_walk_anim_list.blk"
    path:t="gamedata/motion_matching/changed_civilian_mm_attack_anim_list.blk"
  }

  "main_database__pbcWeightOverrides:object"{}
  "main_database__availableTags:list<t>"{
    tag:t="bad"
    tag:t="idle"
    tag:t="prepare_attack"
    tag:t="attack"
  }
}

changed_civilian_mm{
