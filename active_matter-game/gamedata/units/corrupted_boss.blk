    offset:p3=0, 1.5, 0
    haveCollision:b=no

    body{
      offset:p3=0, 0.1, 0
      haveCollision:b=yes
    }
  }
}

climbLadderState{
  walkSpeed:r=1.5
  runSpeed:r=1.5
  sprintSpeed:r=2.25
  rotateAngles:p2=-30, 30
  rotateSpeed:r=20

  collisionLinks{
    offset:p3=0, 0.3, 0
    haveCollision:b=no

    body{
      offset:p3=0, 1, 0
      haveCollision:b=yes
    }
  }
}

