standState{
  walkSpeed:r=1
  runSpeed:r=1
  sprintSpeed:r=1.5
  rotateAngles:p2=-30, 30
  rotateSpeed:r=20

  collisionLinks{
    offset:p3=0.3, 0.8, 0
    haveCollision:b=no

    body{
      offset:p3=0, 0.7, 0
      capsuleRadiusScale:r=1.2
      haveCollision:b=yes
    }
  }
}

crouchState{
  walkSpeed:r=1
  runSpeed:r=1
  rotateAngles:p2=-20, 20
  rotateSpeed:r=20

  collisionLinks{
    offset:p3=0.3, 0.8, 0
    haveCollision:b=no

    body{
      offset:p3=0, 0.3, 0
      capsuleRadiusScale:r=1.2
      haveCollision:b=yes
    }
  }
}

crawlState{
  walkSpeed:r=0.5
  runSpeed:r=0.5
  rotateAngles:p2=-60, 60
  alignSpeed:r=90
  rotateSpeed:r=20
  collisionCenterPos:p3=0.3, 0, 0
  ccdPos:p3=0.2, 0.2, 0

  collisionLinks{
    offset:p3=-0.2, 0.2, 0
    haveCollision:b=no

    body{
      offset:p3=1.1, 0, 0
      capsuleRadiusScale:r=1.2
      haveCollision:b=yes
    }
  }
}

swimState{
  collisionCenterPos:p3=0, -0.7, 0
  walkSpeed:r=1
  runSpeed:r=1.25
  sprintSpeed:r=2.5

  collisionLinks{
    offset:p3=-0.1, -0.5, 0
    haveCollision:b=no

    body{
      offset:p3=0, 0.7, 0
      oriParamMult:r=0.4
      haveCollision:b=yes
    }
  }
}

swimUnderwaterState{
  collisionCenterPos:p3=0, -0.7, 0
  walkSpeed:r=1.25
  runSpeed:r=2
  sprintSpeed:r=3

  collisionLinks{
    offset:p3=-0.1, -0.5, 0
    haveCollision:b=no

    body{
      offset:p3=0, 0.7, 0
      oriParamMult:r=0.4
      haveCollision:b=yes
    }
  }
}

