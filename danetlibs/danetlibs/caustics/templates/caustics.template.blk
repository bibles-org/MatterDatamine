caustics{
  _singleton:b=yes
  _tags:t="render"
  caustics__indoor_probe_shader_name:t="indoor_probe_to_depth"
  needs_water_heightmap:b=no
  combined_shadows__use_additional_textures:b=no

  "caustics__node:dafg::NodeHandle"{}
  "caustics__indoor_probe_mask:UniqueTexHolder"{}
  "caustics__indoor_probe_shader:ShadersECS"{}
  "combined_shadows__additional_textures:list<t>"{
    item:t="caustics_tex"
  }

  "combined_shadows__additional_samplers:list<t>"{
    item:t="caustics_tex_samplerstate"
  }
}

caustics_settings{
  _singleton:b=yes
  _tags:t="render"
  water_caustics_scroll_speed:r=0.75
  water_caustics_world_scale:r=0.25
}
