    _tags:t="sound"
    vehicle_collision_sound__scrapeMinMax:p2=0, 2.5
  }
}

vehicle_damage_sound{
  _group{
    _tags:t="server"
    vehicle_damage_net_sound__cooldown:r=0.05
    vehicle_damage_net_sound__nextTime:r=0
  }

  "vehicle_damage_sound__paths:shared:object"{
    _tags:t="sound"

    "destroy:object"{
      path:t="impact/car_explosion"
    }

    "partDamage:object"{}
    "partDestroy:object"{
      path:t="enemy/vehicle/tank/impacts/part_destroy"
    }

    "partDestroyByFire:object"{
      path:t="enemy/vehicle/tank/impacts/part_destroy_by_fire"
    }
  }
}

tank_vehicle_damage_sound_setup{
  "vehicle_damage_sound__paths:shared:object"{
    _tags:t="sound"

    "destroy:object"{
      path:t="impact/tank_explosion"
    }

    "partDamage:object"{
      path:t="enemy/vehicle/tank/impacts/damage"
    }

    "partDestroy:object"{
      path:t="enemy/vehicle/tank/impacts/part_destroy"
    }

    "partDestroyByFire:object"{
      path:t="enemy/vehicle/tank/impacts/part_destroy_by_fire"
    }
  }
}

vehicle_sound_speed_subtemplate{
  _group{
    _tags:t="sound"
    vehicle_sound_speed_state__linearVelocity:p3=0, 0, 0
    vehicle_sound_speed_state__speed:r=0
    vehicle_sound_speed__prevTransform:m=[[1, 0, 0] [0, 1, 0] [0, 0, 1] [0, 0, 0]]
    vehicle_sound_speed__summary:p4=0, 0, 0, 0
  }
}

vehicle_sound_phys_subtemplate{
  _group{
    _tags:t="sound"
    vehicle_sound_phys_state__isOnGround:b=no
    vehicle_sound_phys_state__deltaShock:r=0
    vehicle_sound_phys_state__noiseReduction:r=0
    vehicle_sound_phys__prevSoundShockSummary:r=0
    vehicle_sound_phys__noiseReductionTimer:r=1
  }
}

vehicle_sound_phys{
  _group{
    _tags:t="sound"
    vehicle_sound_phys__noiseReductionDecayPeriod:r=5
    vehicle_sound_phys__noiseDeltaShockThreshold:r=1
    vehicle_sound_phys__noiseSpeedThreshold:r=0.05
  }
}

tank_vehicle_sound_phys_setup{
