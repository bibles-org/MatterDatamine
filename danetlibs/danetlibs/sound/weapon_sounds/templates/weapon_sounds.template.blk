  _group{
    _tags:t="sound"
    gun_guidance_state_sound__guidanceState:i=-1
    gun_guidance_state_sound__isWatched:b=no
    gun_guidance_state_sound__warmingUpPath:t="/weapon/antivehicle/igla/pzrk_target_prepare"
    gun_guidance_state_sound__activePath:t="/weapon/antivehicle/igla/pzrk_target_active"
    gun_guidance_state_sound__trackingPath:t="/weapon/antivehicle/igla/pzrk_target_locked"

    "gun_guidance_state_sound__event:SoundEvent"{}
